class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        map<int,int>m;
        vector<int>v;
        for(int i=0;i<nums.size();i++){
            m[nums[i]]++;
        }
        for(auto val:m){
                if(val.second == 2){
                    v.push_back(val.first);
                }

        }
        return v;
    }
};
